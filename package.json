{
  "name": "css-sorting",
  "displayName": "Css Sorting",
  "description": "VS Code Css Sorting插件，用于按指定顺序对CSS规则内容进行排序",
  "version": "0.0.5",
  "publisher": "CssSorting",
  "icon": "CssSorting.ico",
  "preview": true,
  "engines": {
    "vscode": "^1.60.0"
  },
  "categories": [
    "Formatters"
  ],
  "activationEvents": [
    "onCommand:extension.formate",
    "onLanguage:css",
    "onLanguage:scss",
    "onLanguage:sass",
    "onLanguage:vue"
  ],
  "main": "./src/extension.js",
  "contributes": {
    "commands": []
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.60.0",
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.2.2",
    "@types/node": "14.x",
    "eslint": "^7.27.0",
    "glob": "^7.1.7",
    "mocha": "^8.4.0",
    "typescript": "^4.3.2",
    "vscode-test": "^1.5.2"
  },
  "dependencies": {
    "prettier-eslint": "^13.0.0",
    "vue-template-compiler": "^2.6.14"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gongbirui/cssAttrSorting.git"
  }
}
